@incollection{arnoldWovenTechniquesSocial2014,
  title = {Woven Techniques and Social Interactions in the {{South Central Andes}} : Ladder Designs and the Visualisation of Productive Output.},
  shorttitle = {Woven Techniques and Social Interactions in the {{South Central Andes}}},
  author = {Arnold, Denise and Espejo, Elvira},
  date = {2014},
  pages = {303-326.},
  abstract = {In this chapter we examine the relations between weaving techniques and the social domain of knowledge and documentation, as expressed through the so-called ‘ladder' techniques, in English terminology. These ladder techniques are characterised by their rows of contrasting colours in the simple form, or alternatively by their intercalated chequered patterns in the multiple form. Commonly they are used today to begin and end a textile design band, but they sometimes constitute whole bands. These ladder techniques have been mentioned in passing by a number of scholars, usually in the context of striped farming bags, both ethnographic and archaeological. However, no one to date has ventured a coherent account of their uses and meanings. We argue here that ladder techniques have played an important articulating role between textile technology and the social domain in the past, above all from the Middle Horizon Period, as an essential element in a coding in cloth to document the storage of food products, and the transactions made to gain access to these products at different ecological altitudes. Evidence is drawn from a range of archaeological examples, and ethnographic examples from ayllu Qaqachaka, ayllu K'ultha, and Sacaca in highland Bolivia, among others.},
  isbn = {978-1-909492-08-0}
}

