@article{mohamedmaherbenismailSurveyContentbasedImage2017,
  title = {A {{Survey}} on {{Content-based Image Retrieval}}},
  author = {{Mohamed Maher Ben Ismail} and Ismail, Mohamed Maher Ben},
  date = {2017},
  journaltitle = {International Journal of Advanced Computer Science and Applications},
  volume = {8},
  number = {5},
